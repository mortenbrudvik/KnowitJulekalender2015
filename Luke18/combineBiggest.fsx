let combineBiggest numbers =
    let comparer (x:string) (y:string) =
        if (x + y |> int) > (y + x |> int) then -1 else 1

    Array.sortWith comparer numbers
    |> Array.reduce (fun acc item -> acc + item)

combineBiggest numbers
|> printfn "%A" 
